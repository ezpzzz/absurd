{
  "name": "absurd",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run build:server",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist-server/server/index.js",
    "lint": "eslint . --ext ts,tsx",
    "test": "jest --coverage",
    "test:e2e": "playwright test",
    "coverage:badge": "coverage-badges",
    "prune": "node scripts/pruneExpiredSites.js",
    "prepare": "npx is-ci || npx husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/icons-material": "^6.0.0",
    "@mui/material": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "groq-sdk": "^0.30.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.0",
    "recharts": "^3.1.2",
    "uuid": "^9.0.0",
    "vite": "^5.0.0",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.2.0",
    "@types/supertest": "^6.0.0",
    "coverage-badges": "^1.0.7",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^9.0.0",
    "is-ci": "^4.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.0.0",
    "msw": "^2.2.14",
    "prettier": "^3.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "optionalDependencies": {
    "@playwright/test": "^1.45.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md}": "prettier --write"
  }
}
